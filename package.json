{
  "name": "browserinc",
  "version": "1.0.0",
  "description": "Change browser fields of dependent packages as a postinstall hook",
  "bin": "./dist/cmd.js",
  "scripts": {
    "build": "babel ./lib -d dist",
    "watch": "babel ./lib -w -d dist",
    "test": "babel-tape-runner test/*.js"
  },
  "author": "Aram Drevekenin",
  "license": "MIT",
  "keywords": [
    "browserify",
    "browserify-utility",
    "package.json",
    "no-browser-field",
    "no-bf",
    "include",
    "exclude"
  ],
  "dependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "findit": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "debounce": "^1.0.0",
    "findit": "^2.0.0",
    "ncp": "^2.0.0",
    "tape": "^4.4.0",
    "tmp": "0.0.28"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imsnif/browserinc"
  }
}
